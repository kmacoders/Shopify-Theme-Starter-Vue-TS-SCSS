{
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": [
      "es6",
      "dom"
    ],
    "outDir": "./dist",
    // "rootDir": "./src",
    "allowJs": true,
    "module": "commonjs",
    "target": "es5",
    "sourceMap": false,
    "moduleResolution": "node",
    "strict": true,
    "experimentalDecorators": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./src/**/*.ts",
    "./src/**/*.vue",
  ],
}
